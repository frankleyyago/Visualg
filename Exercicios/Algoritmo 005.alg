algoritmo "Algoritmo 005"
// Função : Ler dois valores e tirar a media
// Autor : Yago Frankley
// Data : 13/04/2023
var
   n1, n2, media: real
inicio
   escreva("Nota 1: ")
   leia(n1)
   escreva("Nota 2: ")
   leia(n2)
   media <- (n1 + n2) / 2
   escreva("A média entre", n1, " e", n2, " é igual a", media)
fimalgoritmo
